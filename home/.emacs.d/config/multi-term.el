;;;; Multi-Term Configuration
(global-set-key (kbd "C-x <C-up>"    ) `multi-term)
(global-set-key (kbd "C-x <C-right>" ) `multi-term-next)
(global-set-key (kbd "C-x <C-left>"  ) `multi-term-prev)
(global-set-key (kbd "C-x <C-down>"  ) `multi-term-dedicated-toggle)


